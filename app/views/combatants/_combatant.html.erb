<div id="<%= dom_id combatant %>">

<% @combatant.attributes.each do |attribute| %>
    <% next if attribute[0] == "id" %>
    <div class="mb-3 row">
        <label for="staticEmail" class="col-sm-2 col-form-label"><%= attribute[0].humanize%></label>
        <div class="col-sm-10">
            <input type="text" readonly class="form-control-plaintext" id="staticEmail" value=<%=attribute[1]%>>
        </div>
    </div>
<% end %>
       
